<form action="/user/process_login" method="post" id="login" name="login" accept-charset="utf-8">
	<div class="yform-item yform-text" id="username-container">
		<label for="username">Username</label>
		<input type="text" id="username" name="username">
	</div>

	<div class="yform-item yform-password" id="password-container">
		<label for="password">Password</label>
		<input type="password" id="password" name="password">
	</div>

	<div class="yform-item yform-submit" id="search-container">
		<button type="submit">Log in</button>
	</div>
</form>
